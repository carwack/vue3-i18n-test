{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"],
    "types": ["vite/client", "jest", "node", "numeral", "google.maps", "vue", "@vue/runtime-core"],
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.tsx", "src/**/*.vue", "src/main.ts"],
  "exclude": ["node_modules"]
}
